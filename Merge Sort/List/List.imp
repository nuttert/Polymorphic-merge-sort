#ifndef List_imp
#define List_imp
#include "List.hpp"
//List
template<class Element>
NodeOfList<Element>::NodeOfList(Element& element):value(element){}


template<class Element>
List<Element>::List():first(nullptr),last(nullptr){}
    
template<class Element> //1. APPEND
void List<Element>::append(Element& element){
        last  = (first != nullptr ? last->next : first) = new NodeOfList(element);
    }
#endif /* List_hpp */
